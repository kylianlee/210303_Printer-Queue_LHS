#include <iostream>
#define SIZE 100

using namespace std;

void PrinterQueue(int printerQ[SIZE], int targetIndex, int docNum);
void Rearrange(int printerQ[SIZE], int start, int end, int docNum);

int main() {
    int caseNum = 0, docNum = 0, targetIndex = 0;
    int printerQ[SIZE] = {0};

    cin >> caseNum;

    for(int i = 0; i < caseNum; i++){
        cin >> docNum >> targetIndex;
        for(int j = 0; j < docNum; j++){
            cin >> printerQ[j];
        }
        PrinterQueue(printerQ, targetIndex, docNum);
        cout << printerQ[targetIndex + 1];
    }

    return 0;
}

void PrinterQueue(int printerQ[SIZE], int targetIndex, int docNum){
    int updatedMax = printerQ[0];
    int previousMax[SIZE] = {0};
    int j = 0;  //  j = index of previousMax

    for(int i = 1; printerQ[i] != 0; i++){
        if(printerQ[i] > updatedMax) {
            previousMax[j++] = updatedMax;
            updatedMax = printerQ[i];
            Rearrange(printerQ, i, i, docNum);
        }
    }

}

void Rearrange(int printerQ[SIZE], int start, int end, int docNum){
    int temp[SIZE] = {0};
    int amount = docNum - end;

    for(int i = start, j = 0; i <= end; i++, j++){
        temp[j] = printerQ[i];
    }
    for(int i = end + 1, j = start; printerQ[i] != 0; i++, j++){
        printerQ[j] = printerQ[i];
    }
    for(int i = amount, j = 0; i < docNum; i++, j++){
        printerQ[i] = temp[j];
    }
}
